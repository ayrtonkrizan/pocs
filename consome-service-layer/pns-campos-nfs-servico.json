{
   "CardCode" : Codigo do Parceiro - Dependendo da base eh criado automaticamente,
   "CardName" : Razao social,
   "CardType" : C cliente/ S - Fornecedor .... Tambem pode ser usado cCustomer ou cSupplier,
   "Phone1" : Telefone ,
   "Phone2" : DDD,
   "EmailAddress": Email do cidadao,
   "U_SKILL_NFSEEMAIL" : E-Mail para envio da NFSE,
   "BPAddresses" : [
      {
         "AddressName" : Chave do endereco (pode ser repetido entre endereco de cobranca e entrega),
         "Street" : logradouro,
         "Block" : bairro,
         "ZipCode" : cep,
         "City" : cidade,
         "County" : ID Municipio (é uma lista com ids dentro do SAP),
         "Country" : Pais com dois digitos (BR),
         "State" : Estado com dois digitos (SP),
         "BuildingFloorRoom" : Complemento,
         "AddressType" : bo_ShipTo - Endereço de entrega / bo_BillTo - Endereço de cobrança,
         "TypeOfAddress" : Tipo logradouro (Rua, Av ...),
         "StreetNo" : Numero,
         "BPCode" : Codigo PN - Importante no momento da atualização - Na inserção não é necessário
      }
   ],
   "BPPaymentMethods" : [
      {
         "PaymentMethodCode" : Codigo Forma de pagamento - Depende da Base - Necessario enviar para vincular as formas de pagamento com os clientes.
                                 Caso não utilize esse cadastro na base, não é necessário enviar.
      }
   ],
   "BPFiscalTaxIDCollection" : [
      {
         "Address" : Id Do Endereço (tem que estar na lista BPAddresses),
         "CNAECode" : Codigo CNAE,
         "TaxId0" : CNPJ,
         "TaxId1" : IE,
         "TaxId3" : IM,
         "TaxId4" : CPF,
         "BPCode" : Codigo PN - Importante no momento da atualização - Na inserção não é necessário,
         "AddrType" : bo_ShipTo - Endereço de entrega / bo_BillTo - Endereço de cobrança,
      }
   ]
}
